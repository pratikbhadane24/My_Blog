<%- include("partials/header") -%>
    <h1>🙋🏻‍♂️Hello There!!!</h1>
    <p>
        <%= StartingContent %>
    </p>

    <% posts.forEach(function(post){ %>

        <h5>
            <%= post.title %>
        </h5>
        <p>
            <%= post.content.substring(0, 100) + "..." %>
                <a class="inline" href="/posts/<%= post.title %>">Read More</a>
        </p>

        <% }); %>

            <%- include("partials/footer") -%>